  debug(xs, ys, cnt);
  for (int i = 0; i < 5; i++) {
    for (int j = 0; j < 5; j++) {
      if (i == xs[0] && j == ys[0]) {
        cerr << 'A';
      } else
      if (i == xs[1] && j == ys[1]) {
        cerr << 'B';
      } else {
        cerr << a[i][j];
      }
      cerr << " \n"[j == 4];
    }
  }
  cerr << '\n';
